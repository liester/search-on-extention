{
  "manifest_version": 2,
  "name": "Proxibid Search",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "pbsearchicon.png"
  },
  "background": {
    "scripts": [
      "eventPage.js"
    ],
    "persistence": false
  },
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "pbsearchicon.png",
    "48": "pbsearchicon.png",
    "128": "pbsearchicon.png"
  }
}